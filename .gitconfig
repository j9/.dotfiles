[core]
  pager = less -FRSX
[user]
  name = Janis Gengeris
  email = janis.gengeris@gmail.com
[color]
  ui = auto
  branch = auto
  diff = auto
  status = auto
[alias]
  ci = commit -a -m
  co = checkout
  st = status
  df = diff
  br = branch
  lastlog = log --pretty=format:'%Cblue%h%Creset c:%Cred%cn%Creset a:%Cred%an%Creset %Cgreen%s%Creset %ar' -n 10
  who = shortlog -e -s -n 
[merge]
  conflictstyle = diff3
[include]
  path = ~/.gitconfig.github
